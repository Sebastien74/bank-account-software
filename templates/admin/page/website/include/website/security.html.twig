{% trans_default_domain 'admin' %}

<h4 class="card-subtitle mb-3">{{ 'fal cog'|icon(null, 18, 'info-darken me-2') }}{{ "Configuration"|trans }}</h4>
<div class="row">
    {{ form_row(form.security.headerData) }}
    {{ form_row(form.security.secureWebsite) }}
    {{ form_row(form.security.resetPasswordsByGroup) }}
</div>

<h4 class="card-subtitle mb-3">{{ 'fal network-wired'|icon(null, 18, 'info-darken me-2') }}{{ "Administrateurs"|trans }}</h4>
<div class="row">
    {{ form_row(form.security.adminRegistration) }}
    {{ form_row(form.security.adminRegistrationValidation) }}
    {{ form_row(form.security.adminPasswordSecurity) }}
    {{ form_row(form.security.adminPasswordDelay) }}
</div>